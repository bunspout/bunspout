{
  "name": "bunspout",
  "main": "index.ts",
  "module": "index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./index.ts",
      "default": "./index.ts"
    }
  },
  "version": "0.0.1-alpha.1",
  "keywords": [
    "excel",
    "xlsx",
    "xls",
    "spreadsheet",
    "read",
    "write",
    "streaming",
    "bun",
    "typescript"
  ],
  "files": [
    "index.ts",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.bench.ts",
    "!src/scripts/**",
    "README.md",
    "tsconfig.json"
  ],
  "scripts": {
    "bench": "for file in src/tests/benchmarks/*.bench.ts; do bun --expose-gc run \"$file\"; done",
    "lint": "bun run typecheck && eslint .",
    "lint:fix": "eslint . --fix",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "buffer-crc32": "^1.0.0",
    "date-fns": "^3.0.0",
    "fd-slicer": "^1.1.0",
    "sax": "^1.3.0",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/bun": "latest",
    "@types/eslint": "^9.6.1",
    "@types/yazl": "^2.4.3",
    "@types/yauzl": "^2.10.3",
    "@types/sax": "^1.2.7",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.15.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "tinybench": "^2.7.0",
    "typescript-eslint": "^8.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "author": "Alexander Hofstede",
  "license": "MIT",
  "packageManager": "bun@^1.3.5",
  "engines": {
    "bun": ">=1.3.5",
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:aphofstede/bunspout.git"
  },
  "bugs": {
    "url": "https://github.com/aphofstede/bunspout/issues"
  },
  "homepage": "https://github.com/aphofstede/bunspout#readme"
}
